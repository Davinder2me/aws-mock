{
    "name": "aws-mock",
    "version": "1.0.1",
    "description": "Easily and automatically test essential AWS services with the language-agnostic aws-mock.",
    "main": "build.gradle",
    "devDependencies": {
        "aws-sdk": "~2.0.0",
        "chai": "~1.8.1",
        "yahoo-arrow": "~0.2.0",
        "http-proxy": "~0.10.3"
    },
    "scripts": {
        "test": "./node_modules/yahoo-arrow/node_modules/.bin/istanbul cover ./node_modules/yahoo-arrow/node_modules/.bin/_mocha ./example/nodejs/test/TestExamples.js -- -R spec; ./node_modules/yahoo-arrow/node_modules/.bin/istanbul check-coverage --statements 85 --functions 85 --branches 85 --lines 85"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/treelogic-swe/aws-mock.git"
    },
    "keywords": [
        "aws",
    "mock",
    "ec2"
        ],
    "author": {
        "name": "treelogic-swe.com"
    },
    "license": "Apache",
    "readmeFilename": "README.md",
    "_id": "aws-mock@1.0.1",
    "_from": "aws-mock@"
}
